ul{ display: block;}
ul li{ display: block;}
view{ overflow: hidden;}
.nav{ width: 100%; height: 100rpx; position: fixed; top: 0; z-index: 1000;}
.week{ height: 100%; width: 100%; background: #f6f6f6;}
.week li{ width: 13%; height: 100%; float: left; text-align: center; box-sizing: border-box; line-height: 100rpx;}
.week li.on{ color:#fff; background: #ffba00;}
.content{ position: absolute; top: 100rpx; height: 1560rpx; width: 100%; background: #ffffff;}
.colomn{ width: 67.5rpx; float: left;}
.colomn li{ width: 67.5rpx; text-align: center; height: 120rpx; line-height: 120rpx; box-sizing: border-box; border-bottom: 2rpx solid #eee; border-right: 1rpx solid #eee;}
.class{ width: 100%;}
.class li{ width: 13%; float: left; text-align: center; height: 120rpx; line-height: 120rpx; box-sizing: border-box; border-bottom: 2rpx solid #eee; border-left: 1rpx solid #eee; border-right: 1rpx solid #eee; padding: 1px;}
.class li view{ width: 100%; height: 100%; background: gray;}